{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@components.react": ["src/components.react"],
      "@components.react/*": ["src/components.react/*"],
      "@modules/*": ["src/modules/*"],
      "@assets/*": ["src/assets/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["src/styles/*"],
      "@i18n/*": ["src/i18n/*"],
      "@types": ["src/types"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
